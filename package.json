{
  "name": "@russ-b/nestjs-common-tools",
  "version": "1.1.2",
  "description": "NestJS and TypeORM utility tools",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsc",
    "test": "jest",
    "prepare": "npm run build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "nestjs",
    "typeorm",
    "repository",
    "database"
  ],
  "author": "RuSS-B",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RuSS-B/nestjs-common-tools.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./grpc/transformers": "./dist/common/grpc/transformers/index.js",
    "./validators": "./dist/common/grpc/validators/index.js"
  },
  "typesVersions": {
    "*": {
      "grpc/transformers": ["dist/grpc/transformers/index.d.ts"]
    }
  },
  "peerDependencies": {
    "@grpc/grpc-js": "^1.12.5",
    "@nestjs/common": "^11.0.0",
    "@nestjs/microservices": "^11.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "protobufjs": "^7.4.0",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-proto": "^2.6.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
