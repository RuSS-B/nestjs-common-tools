{
  "name": "@russ-b/nestjs-common-tools",
  "version": "1.10.3",
  "description": "NestJS utility tools",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsc",
    "test": "jest",
    "prepare": "npm run build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "nestjs",
    "typeorm",
    "database",
    "tools"
  ],
  "author": "RuSS-B",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RuSS-B/nestjs-common-tools.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./common/util": "./dist/common/util/index.js",
    "./common/pagination": "./dist/common/pagination/index.js",
    "./common/filters": "./dist/common/filters/index.js",
    "./validators": "./dist/validators/index.js",
    "./modules": "./dist/modules/index.js",
    "./logger": "./dist/logger/index.js"
  },
  "typesVersions": {
    "*": {
      "validators": [
        "dist/validators/index.d.ts"
      ],
      "common/util": [
        "dist/common/util/index.d.ts"
      ],
      "common/pagination": [
        "dist/common/pagination/index.d.ts"
      ],
      "common/filters": [
        "dist/common/filters/index.d.ts"
      ],
      "modules": [
        "dist/modules/index.d.ts"
      ],
      "logger": [
        "dist/logger/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "uuid": "^11.0.5",
    "winston-loki": "^6.1.3"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/lib-storage": "^3.0.0",
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/config": "^3.0.0 || ^4.0.0",
    "nest-winston": "^1.10.2",
    "protobufjs": "^7.4.0",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/testing": "^11.0.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-proto": "^2.6.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
